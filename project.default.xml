            <option name="processCode" value="true" />
            <option name="processLiterals" value="true" />
            <option name="processComments" value="false" />
          </inspection_tool>
        </profile>
        <option name="PROJECT_PROFILE" />
        <option name="USE_PROJECT_PROFILE" value="false" />
